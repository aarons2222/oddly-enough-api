{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "continue": true
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-refresh?manual=true",
      "schedule": "0 8 * * *"
    }
  ]
}
